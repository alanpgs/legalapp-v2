<script>
    const filterSize = () => {
        var x = document.querySelector('.laws_filter').offsetHeight;
        document.body.style.setProperty('--filter-height', x + "px");
    }

    var swiperCount = document.querySelectorAll('.placeholder_ .swiper-slide').length;

    var swiperLength = swiperCount > 12 ? 12 : swiperCount;

    var swiperDir = window.innerWidth < 1200 ? 'horizontal' : 'vertical';

    const getyearValue = (target, callback) => {
        var t = document.querySelectorAll('.placeholder_ input[type=radio]');

        t.forEach((el, indx) => {
            el.addEventListener('click', () => {
                callback(el.value, indx)
            })
        });
    }

    window.addEventListener('DOMContentLoaded', () => {

        $('.tooltip').tooltipster();

        const select1 = new SlimSelect({
            select: '#select1'
        })

        const select2 = new SlimSelect({
            select: '#select2'
        })

        filterSize();


        var dateSwiper = new Swiper(".placeholder_ .swiper-container", {
            slidesPerView: window.innerWidth < 768 ? 5 : 6,
            spaceBetween: 10,
            direction: swiperDir,
            preventClicks: true,
            a11y: false,
            scrollbar: {
                el: '.placeholder_ .swiper-scrollbar',
                draggable: true,
                hide: true,
            },
            navigation: {
                nextEl: '.placeholder_ .navBox.right',
                prevEl: '.placeholder_ .navBox.left'
            },
            mousewheel: {
                invert: false,
            },
            breakpoints: {
                768: {
                    slidesPerView: 8,
                    spaceBetween: 10,
                },
                1200: {
                    slidesPerView: 10,
                    spaceBetween: 15,
                },
            },
        });

        getyearValue('.placeholder_ input[type=radio]', (value, index) => {
            console.log(value);
            //dateSwiper.slideTo(index);
        })




    })

    window.addEventListener('resize', () => {
        filterSize();
    })

</script>