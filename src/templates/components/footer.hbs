<script>

  const footerHeight = document.querySelector('footer').offsetHeight;

  const ratingFunc = (target, callback)=> {
  
    const T_ = document.querySelector(target);
    const TI_ = T_.querySelectorAll('input[type=checkbox]');
    const LI_ = T_.querySelectorAll('li');
    let array = new Array();
    let v = 0;

    TI_.forEach((el,index)=> {

      if(el.checked){
        for(let x = 0; x <= index; x++){
          LI_[x].classList.add('active_')
        }
      }

      array.push(LI_[index])

      el.onclick = (e)=> {

        array.map((item) => {item.classList.remove('active_')});

        v = e.target.value;
        let m = v - 1;
        
        for(let x = 0; x <= m; x++){
          LI_[x].classList.add('active_')
        }

        if (typeof callback == "function")
        callback(v);

      }
    })
  };

  window.addEventListener('DOMContentLoaded', ()=> {

    document.body.style.setProperty('--footer-height', footerHeight + "px");

    ratingFunc('[data-star-rating]', function(value) {
      console.log(value)
    })
  })

  
</script>
